{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/pt-br/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/$",
			"dest": "/_isr?x_astro_path=$0"
		}
	]
}